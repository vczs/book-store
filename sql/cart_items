CREATE TABLE `cart_items` (
  `id` int NOT NULL AUTO_INCREMENT,
  `count` int NOT NULL,
  `amount` double(11,2) NOT NULL,
  `book_id` int NOT NULL,
  `cart_id` varchar(100) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `cart_id` (`cart_id`),
  KEY `book_id` (`book_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;

